{
  "version": "0.1.0",
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsdx watch --onSuccess \"node dist/index.js\"",
    "migrate": "node -r dotenv/config  ./database/migrate.js",
    "watch": "tsdx watch",
    "build": "tsdx build",
    "test": "tsdx test",
    "lint": "tsdx lint",
    "prepare": "tsdx build",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "peerDependencies": {},
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "name": "server",
  "author": "Michael McQuade",
  "module": "dist/server.esm.js",
  "size-limit": [
    {
      "path": "dist/server.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/server.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^4.12.0",
    "@types/express": "^4.17.12",
    "dotenv": "^10.0.0",
    "husky": "^6.0.0",
    "prisma": "^2.25.0",
    "size-limit": "^4.12.0",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "meyer": "^1.2.2",
    "meyer-dbms-knex": "^1.0.0",
    "pg": "^8.6.0",
    "pg-connection-string": "^2.5.0"
  }
}
